{
    "name": "inspheric/nova-defaultable",
    "description": "Default values for Nova fields when creating resources and running resource actions.",
    "keywords": [
        "laravel",
        "nova",
        "field",
        "default",
        "prepopulate",
        "default value",
        "cache",
        "action",
        "create",
        "update"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "<3.4.0"
    },
    "autoload": {
        "psr-4": {
            "Inspheric\\NovaDefaultable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Inspheric\\NovaDefaultable\\DefaultableServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
